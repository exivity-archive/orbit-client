{
  "name": "orbit-client",
  "version": "0.0.28",
  "description": "React client for orbitjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exivity/orbit-client.git"
  },
  "keywords": [
    "exivity",
    "orbit-client"
  ],
  "author": "Michiel de Vos <Michiel_87@hotmail.com>",
  "bugs": {
    "url": "https://github.com/exivity/orbit-client/issues"
  },
  "homepage": "https://github.com/exivity/orbit-client",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "orbit-client": "./src/utils/createEntities.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "lint": "standard src",
    "lint:fix": "standard src --fix",
    "test": "jest || true",
    "start": "start-storybook -p 6007",
    "build": "yarn build:clean && yarn build:dist && yarn build:docs",
    "build:clean": "rimraf dist && rimraf docs",
    "build:dist": "babel src --out-dir dist --source-maps --ignore \"src/**/*.stories.js\",\"src/**/*.test.js\"",
    "build:docs": "build-storybook -o docs",
    "publish:docs": "yarn build:clean && yarn build:docs && git add docs && git commit -m \"chore: update docs\" && git subtree push --prefix docs origin gh-pages",
    "prepublishOnly": "yarn lint yarn&& yarn test && yarn build",
    "build:entities": "node ./src/orbit-client/utils/createStoryBookEntities.js",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@exivity/ui": "^0",
    "@storybook/addon-actions": "^4.0.0-alpha.24",
    "@storybook/addon-info": "^4.0.0-alpha.24",
    "@storybook/addon-knobs": "^4.0.0-alpha.24",
    "@storybook/addon-links": "^4.0.0-alpha.24",
    "@storybook/addon-options": "^4.0.0-alpha.24",
    "@storybook/addons": "^4.0.0-alpha.24",
    "@storybook/react": "^4.0.0-alpha.24",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "docz": "^0.12.9",
    "docz-theme-default": "^0.12.9",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-json-view": "^1.19.1",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "@orbit/coordinator": "^0.15.23",
    "@orbit/data": "^0.15.23",
    "@orbit/store": "^0.15.23",
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.11",
    "pluralize": "^7.0.0",
    "prismjs": "^1.15.0",
    "react-orbitjs": "^0.1.10"
  },
  "optionalDependencies": {},
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
